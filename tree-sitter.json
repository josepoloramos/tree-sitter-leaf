{
  "grammars": [
    {
      "name": "leaf",
      "camelcase": "Leaf",
      "scope": "source.leaf",
      "path": ".",
      "external-files": ["src/tag.h"],
      "file-types": ["leaf"],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "injection-regex": "leaf"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Leaf template language grammar for tree-sitter",
    "authors": [
      {
        "name": "Jose Polo Ramos",
        "email": "jose@jpoloram.com"
      }
    ],
    "links": {
      "repository": "https://github.com/josepoloramos/tree-sitter-leaf"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
